{
  "name": "production",
  "description": "Production",
  "chef_type": "environment",
  "json_class": "Chef::Environment",
  "default_attributes": {
    "db": {
      "host": "db1-internal.myplaceonline.com",
      "dbname": "myplaceonline_production",
      "dbuser": "myplaceonline"
    },
    "app": {
      "name": "myplaceonline",
      "server_name": "myplaceonline.com",
      "server_protocol": "https",
      "smtp_user": "myplaceonline",
      "mail_from": "contact@myplaceonline.com",
      "root_email": "root@myplaceonline.com",
      "tmpdir": "/var/tmp/",
      "full_text_search_target": "db2-internal.myplaceonline.com:9200"
    },
    "sslmate": {
      "certname": "www.myplaceonline.com"
    },
    "nginx": {
      "user": "nginx",
      "default_site_enabled": false,
      "gzip_static": "on",
      "passenger": {
        "install_method": "source",
        "version": "5.0.26",
        "gem_binary": "/usr/bin/gem",
        "ruby": "/usr/bin/ruby",
        "root": "/usr/local/share/gems/gems/passenger-5.0.26/"
      },
      "source": {
        "version": "1.9.12",
        "checksum": "1af2eb956910ed4b11aaf525a81bc37e135907e7127948f9179f5410337da042",
        "_comment": "wget http://nginx.org/download/nginx-1.9.12.tar.gz; sha256sum nginx-1.9.12.tar.gz;",
        "use_existing_user": true,
        "modules": [
          "nginx::ipv6",
          "nginx::http_ssl_module",
          "nginx::http_gzip_static_module",
          "nginx::passenger",
          "nginx::upload_progress_module"
        ]
      }
    },
    "postgresql": {
      "version": "9.5",
      "replication_cluster": "cluster1"
    },
    "keys": {
      "postgresql": {
        "public": "ssh-rsa "
      }
    },
    "nfs": {
      "client": {
        "mount": "/var/lib/remotenfs/",
        "mount_backup": "/var/lib/remotenfs_backup/",
        "host": "db1-internal.myplaceonline.com"
      },
      "server": {
        "directory": "/var/lib/nfsshare/"
      }
    },
    "influxdb": {
      "url": "http://admin-internal.myplaceonline.com:8086"
    }
  },
  "override_attributes": {
  },
  "cookbook_versions": {
  }
}
